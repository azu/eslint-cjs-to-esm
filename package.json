{
  "name": "eslint-cjs-to-esm",
  "version": "1.0.0",
  "description": "ESLint wrapper for migration from CJS to ESM.",
  "keywords": [
    "eslint",
    "cjs",
    "esm",
    "mjs",
    "migration",
    "autofix",
    "migrate"
  ],
  "homepage": "https://github.com/azu/eslint-cjs-to-esm",
  "bugs": {
    "url": "https://github.com/azu/eslint-cjs-to-esm/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/eslint-cjs-to-esm.git"
  },
  "license": "MIT",
  "author": "azu",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./eslint-wrapper-bin.js",
    "./.eslintrc.cjs": "./.eslintrc.cjs",
    "./package.json": "./package.json"
  },
  "main": "eslint-wrapper-bin.js",
  "files": [
    "eslint-wrapper-bin.js",
    ".eslintrc.cjs"
  ],
  "scripts": {
    "build": "tsc -p .",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "prepare": "git config --local core.hooksPath .githooks",
    "prepublishOnly": "npm run build",
    "test": "mocha",
    "watch": "tsc -p . --watch"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "none"
  },
  "devDependencies": {
    "prettier": "^2.8.3"
  },
  "packageManager": "yarn@1.22.19",
  "dependencies": {
    "eslint": "^8.32.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^45.0.2",
    "execa": "^6.1.0"
  }
}
