{
  "name": "eslint-cjs-to-esm",
  "version": "2.0.0",
  "description": "ESLint wrapper for migration from CJS to ESM.",
  "keywords": [
    "eslint",
    "cjs",
    "esm",
    "mjs",
    "migration",
    "autofix",
    "migrate"
  ],
  "homepage": "https://github.com/azu/eslint-cjs-to-esm",
  "bugs": {
    "url": "https://github.com/azu/eslint-cjs-to-esm/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/eslint-cjs-to-esm.git"
  },
  "license": "MIT",
  "author": "azu",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./eslint-cjs-to-esm.js",
    "./.eslintrc.cjs": "./.eslintrc.cjs",
    "./package.json": "./package.json"
  },
  "main": "eslint-cjs-to-esm.js",
  "bin": {
    "eslint-cjs-to-esm": "./eslint-cjs-to-esm.js"
  },
  "files": [
    "eslint-cjs-to-esm.js",
    ".eslintrc.cjs"
  ],
  "scripts": {
    "test": "expected-exit-status 1 --command 'node eslint-cjs-to-esm.js \"./test-fixtures/*\"'"
  },
  "devDependencies": {
    "expected-exit-status": "^2.0.0"
  },
  "packageManager": "npm@9.7.1",
  "dependencies": {
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.32.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-file-extension-in-import-ts": "^1.0.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "execa": "^7.1.1"
  }
}
